<?xml version="1.0" encoding="UTF-8"?>  

<widgwt xmlns = "http://www.w3.org/ns/widgets"
 xmlns:gap = "http://phonegap.com/ns/1.0"
  id = "com.phonegap.app1"
  version = "1.0.0">
  
  <name> app1-2019 Datos del Dispositivo </name>
  
  <description>
  Aplicacion que nos muestra la informacion referente al dispositivo Movil
  </description>
  
  <author href="https://build.phonegap.com" email="emmajuarezxelhua@gmail.com">
  Guadalupe Juarez Xelhua
  </author>
  
  <!--Personaliza la aplicacion y plataforma -->
  <preference name="orientation"  value="default" />
  <!--<preference name="orientation"  value="portrait" />
  <preference name"orientation"  value="landscape" /> -->
  <preference name="target-device"  value="universal" />
  <preference name="fullscreen"  value="true" />
  <preference name="android-istallLocation"  value="auto" />
  <preference name="permissions"  value="INTERNET" />
  
  <!--plugins-->
  <plugin name="cordova-plugin-device" spec="~1.1.4" />
  <plugin name="cordova-plugin-vibration" spec="~2.1.3" />
  <plugin name="cordova-plugin-dialogs" spec="~1.3.2" />
  
  <!-- <plugin name="cordova-plugin-geolocation" sourse="npm" />-->
  
  
  <!--Colocar icono app -->
  
  <icon src="res/android/mdpi.png" />
  
  </widgwt>
  